name: Testing comment
on: 
    pull_request: 
        paths: 
        - starkmatt/**
jobs:
    comment:
        name: Commenter
        runs-on: ubuntu-latest
        steps:
            - uses: koushikmohan1996/action-pr-gifs@V1.1
              with:
                  github-token: ${{ secrets.SM_GIT_HUB_TOKEN }}
                 
